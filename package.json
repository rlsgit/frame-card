{
  "name": "frame-card",
  "version": "0.1.0",
  "description": "Lovelace custom:frame-card",
  "keywords": [
    "home-assistant",
    "homeassistant",
    "hass",
    "automation",
    "lovelace",
    "custom-cards"
  ],
  "module": "frame-card.js",
  "repository": "https://github.com/rlsgit/frame-card.git",
  "author": "Robert Stephens <robert@rmsei.com>",
  "license": "MIT",
  "scripts": {
